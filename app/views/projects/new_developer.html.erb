<% provide(:title,"Add developer") %>
<h1>Add developer</h1>
<div class="row">
  <div class="col-md-4 col-md-offset-4 rieng3">
    <%= form_for(@project, url: "add_developer", method: :patch) do |f| %>
      <% users = User.all %>
      <%= f.label :assign_id %>
      <%= f.select :assign_id, options_for_select(users.map{ |us| [us.account, us.id] }), class: 'form-control' %>
      
      <%= f.hidden_field :project_id, :value => @project.id,class: 'form-control' %>
      
      <%= f.submit "Add developer", class: "btn btn-info rieng2" %>
    <% end %>
  </div>
</div>

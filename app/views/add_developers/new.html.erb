<% provide(:title,"Add developer") %>
<h1>Add developer</h1>
<div class="row">
  <div class="col-md-4 col-md-offset-4">
  	<% @project = Project.find(params[:id]) %>
    <%= form_for(@relation, url: "/projects/#{@project.id}/add_developers", method: :post) do |f| %>
    <% users = User.all %>
		<%= f.label :add_developer %>
    <%= f.select :assign_id, options_for_select(users.map{ |us| [us.account, us.id] }), class: 'form-control' %>

    <%= f.hidden_field :project_id, :value => @project.id %>
    <%= f.submit "Add developer", class: "btn btn-info str" %>
    <% end %>
  </div>
</div>